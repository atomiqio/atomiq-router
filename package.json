{
  "name": "atomiq",
  "version": "0.0.11",
  "description": "The development version of the template for atomiq microservices",
  "main": "dist/lib",
  "config": {
    "debug": "app*",
    "image": "app",
    "name": "app-ctr",
    "start_cmd": "node ./dist/app.js",
    "debug_cmd": "node-debug --no-preload --web-host 0.0.0.0 ./dist/app.js"
  },
  "scripts": {
    "test": "mocha dist/test",
    "watch": "DEBUG=$npm_package_config_debug babel --watch src -d dist --source-maps",
    "kill-watch": "pkill -f babel",
    "clean": "rm -rf ./dist",
    "build": "npm run babel-build",
    "babel-build": "npm run clean && DEBUG=$npm_package_config_debug babel src -d dist --source-maps",
    "babel-build-inline-sourcemaps": "npm run clean && babel src -d dist --source-maps inline"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "body-parser": "^1.15.0",
    "debug": "^2.2.0",
    "logster": "0.0.5",
    "ramda": "^0.19.1"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "esformatter": "^0.9.2",
    "esformatter-collapse-objects": "^0.5.1",
    "eslint": "^2.3.0"
  }
}
